<!DOCTYPE html>
<html>
	<% include head.ejs %>
<body>	

	<article id="images">
	
		<% for (var i = 0; i < detail.img.length; i++) { %>
			
			<% if (detail.img[i]) { %>
				<img src="<%= detail.img[i] %>">
			<% } %>
			
		<% } %>
	
	</article>

	<script>
		if ('serviceWorker' in navigator) {
			navigator.serviceWorker.register('sw.js');
		}
	</script>
	<script src="js/imageScript.js"></script>
</body>
</html>

